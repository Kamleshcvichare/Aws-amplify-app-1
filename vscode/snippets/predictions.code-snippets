    "Amplify Predictions TextToSpeech": {
        "prefix": "Predict.textToSpeech",
        "body": [
            "Predictions.convert({",
                "\ttextToSpeech: {",
                  "\t\tsource: {",
                    "\t\t\ttext: ${1: textToGenerateSpeech},",
