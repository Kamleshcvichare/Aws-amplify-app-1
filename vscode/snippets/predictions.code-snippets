    "Amplify Predictions Transcription": {
        "prefix": "Predict.transcription",
        "body": [
            "Predictions.convert({",
                "\ttranscription: {",
                  "\t\tsource: {",
                    "\t\t\t${1: bytes}",
                  "\t\t},",
                  "\t\t// language: \"en-US\", // other options are \"en-GB\", \"fr-FR\", \"fr-CA\", \"es-US\"",
                "\t}",
              "})",
              ".then(({ transcription: { fullText } }) => console.log({ fullText }))",
              ".catch(err => console.log({ err }));"
        ]
