{
  "name": "aws-amplify-angular",
  "version": "2.1.7-unstable.7",
  "description": "AWS Amplify Angular Components",
  "main": "dist/bundles/aws-amplify-angular.umd.js",
  "module": "dist/fesm5/aws-amplify-angular.js",
  "es2015": "dist/fesm2015/aws-amplify-angular.js",
  "esm5": "dist/esm5/aws-amplify-angular.js",
  "esm2015": "dist/esm2015/aws-amplify-angular.js",
  "fesm5": "dist/fesm5/aws-amplify-angular.js",
  "fesm2015": "dist/fesm2015/aws-amplify-angular.js",
  "typings": "dist/aws-amplify-angular.d.ts",
  "metadata": "aws-amplify-angular.metadata.json",
  "sideEffects": false,
  "scripts": {
    "cleantemp": "rimraf coverage dist tmp docs",
    "ngcompile": "node_modules/.bin/ngc -p tsconfig-aot.json",
    "test": "jest --maxWorkers 2",
    "transpile": "ngc",
    "package": "rollup -c rollup.config.js",
    "minify": "uglifyjs dist/bundles/aws-amplify-angular.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/aws-amplify-angular.umd.min.js",
    "build": "ng-packagr -p package.json && npm run copyassets",
    "copyassets": "copyfiles --error --soft --flat ./src/components/interactions/chatbot/aws-lex-audio.js dist/fesm5",
    "declarations": "ngc --p ./declarations/"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!@ionic)"
    ]
  },
  "ngPackage": {
    "whitelistedNonPeerDependencies": [
      "."
    ]
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "devDependencies": {
    "@angular/common": "^5.2.6",
    "@angular/compiler": "^5.2.9",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/core": "^5.2.6",
    "@angular/forms": "^6.0.3",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@types/jest": "^23.1.5",
    "@types/node": "^9.4.6",
    "@types/paho-mqtt": "^1.0.3",
    "@types/zen-observable": "^0.5.3",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^4.0.1",
    "babel-core": "^6.26.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "copyfiles": "^2.1.0",
    "css-loader": "^0.28.1",
    "jest": "^23.4.0",
    "jest-preset-angular": "^5.2.3",
    "json-loader": "^0.5.4",
    "lodash.has": "^4.5.2",
    "ng-packagr": "^4.7.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "rxjs": "^6.2.1",
    "rxjs-compat": "^6.2.1",
    "source-map-explorer": "^1.3.3",
    "to-string-loader": "^1.1.5",
    "uglify-js": "^3.4.7",
    "zone": "^0.3.4",
    "zone.js": "^0.8.26"
  },
  "dependencies": {
    "@angular/cli": "^7.3.0"
  }
}
